		$ad.style.boxShadow = '2px 10px 18px #00000030'
                $ad.style.zIndex = '1000000'
                $ad.id = 'testad'
                $adCaptionLink.appendChild($ad);

                let insertedNode = ele.insertBefore($adCaptionLink, ele.childNodes[2])

